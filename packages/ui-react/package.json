  "license": "MIT",
  "main": "dist/orbit-ui-react.js",
  "module": "dist-esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build-esm && npm run build-lib && npm run build-storybook",
    "build-esm": "rm -fr ./dist-esm && babel -d dist-esm src",
    "build-lib": "rm -fr ./dist && webpack --mode production",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext js,jsx src",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "start": "storybook-server -p 8081",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "7.2.0"
  },
