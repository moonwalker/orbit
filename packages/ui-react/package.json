{
  "name": "@moonwalker/orbit-ui-react",
  "version": "1.8.0",
  "description": "React UI components library",
  "keywords": [
    "css",
    "components",
    "reactjs"
  ],
  "homepage": "https://github.com/moonwalker/orbit",
  "repository": "moonwalker/orbit",
